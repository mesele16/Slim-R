{
  "name": "ricardoper/slim3-skeleton",
  "type": "library",
  "license": "MIT",
  "description": "PHP Slim Framework v3.5 Skeleton With Twig Views, Flash Messages, VarDumper And A Good Folder Organization",
  "keywords": [
    "PHP",
    "Slim",
    "Framework",
    "Skeleton",
    "Twig",
    "Flash",
    "VarDumper"
  ],
  "homepage": "https://github.com/ricardoper",
  "authors": [
    {
      "name": "Ricardo Pereira",
      "email": "github@ricardopereira.es",
      "homepage": "http://ricardopereira.es"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "slim/slim": "^3.5",
    "slim/twig-view": "^2.1",
    "slim/flash": "^0.4.0",
    "slim/csrf": "^1.3.0",
    "vlucas/phpdotenv": "^5.5.0",
    "symfony/var-dumper": "^6.2.5",
    "illuminate/database": "^10.0.3",
    "awurth/slim-validation": "^3.1",
    "dflydev/fig-cookies": "^3.0",
    "bryanjhv/slim-session": "~3.0",
    "jasongrimes/paginator": "^1.0",
    "bacon/bacon-qr-code": "^2.0.8",
    "interficieis/php-barcode": "^2.0.2",
    "monolog/monolog": "^1.21"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "bootstrap/helpers.php"
    ]
  },
  "config": {
    "optimize-autoloader": true
  }
}
