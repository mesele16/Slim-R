{
    "name": "edux/edux",
    "type": "library",
    "license": "MIT",
    "description": "A better Slim Framework 3 Based students management tool with a sane folder structure where everything is customizable",
    "keywords": [
        "PHP",
        "Slim",
        "Framework",
        "Skeleton"
    ],
    "homepage": "https://github.com/edux",
    "authors": [
        {
            "name": "Mesele Shishay",
            "email": "meseleshishay23@gmail.com",
            "homepage": "http://edux.com"
        }
    ],
    "require": {
    "php": ">=5.5.0",
    "psr/log": "^1.1",
    "slim/slim": "^4.5",
    "slim/psr7": "^1.1",
    "pimple/pimple": "^3.3",
    "vlucas/phpdotenv": "^4.1"
    "slim/twig-view": "^2.1",
    "slim/flash": "^0.4.0",
    "slim/csrf": "^1.3.0",
    "symfony/var-dumper": "^5.2",
    "illuminate/database": "^7.3",
    "awurth/slim-validation": "^3.1",
    "jasongrimes/paginator": "^1.0",
    "bacon/bacon-qr-code": "^2.0.8",
    "interficieis/php-barcode": "^2.0.2",
    "monolog/monolog": "^1.21"
  },

    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Kernel/Helpers/helpers.php",
            "app/Kernel/Helpers/arrayHelpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true
    }
}
